{
    "name": "vbespalov/laravel-telegram",
    "description": "Telegram library for laravel",
    "keywords": [
        "vbespalov",
        "telegram",
        "laravel-telegram"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vbespalov\\LaravelTelegram\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Volodymyr Bespalov",
            "email": "laravel-telegram@vab.com.ua"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "spatie/laravel-data": "^4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vbespalov\\LaravelTelegram\\TelegramServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable" : true
}
